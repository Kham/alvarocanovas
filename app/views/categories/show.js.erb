$("#content").fadeOut('slow',
  function(){
    $('#side_content')
      .html('<%= render_j :partial => 'menu'%>');
    $('#main_content')
      .html('<%= render_j :partial => 'category' %>');
    $(this).fadeIn(function(){
      SlideShow.launchWithDelay('<%= category_pictures_url(@category)%>');
    });

  }
);


